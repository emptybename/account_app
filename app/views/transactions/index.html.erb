<% @transactions.each do |t| %>
	<%= t.transaction_type %>
	<%= t.amount %>
	<%= t.from_account.name %>
	<%= t.to_account.name if t.transaction_type == "transfer" %>
		<%#= t.to_account.name %>
	<%= t.tag_type unless t.tag_type.blank? %>
	<%= t.description unless t.description.blank? %>
	<%= t.date_of_transaction %>
	<%= link_to "edit", edit_transaction_path(t) %>
	<%= link_to "delete", t, method: :delete %><br>
<% end %>