<%= form_for @transaction do |f| %>
	<ul class="nav navbar-nav transaction_form">
	<li><%= f.select(:transaction_type, options_for_select(["insert",
			"delete","transfer"],@transaction.transaction_type),{prompt: "Transaction_type" }) %></li>
	<li><%= f.select(:from_account_id, options_for_select(@account_map, @transaction.from_account_id),{prompt: "From_account"})%></li>
	<li id="to_account_id"><%= f.select(:to_account_id, options_for_select(@account_map,@transaction.to_account_id),{prompt:"To_account"}, )%></li>
	<li><%= f.number_field :amount, step: 'any', placeholder: "amount"%></li>
	<li><%= f.text_field :description, placeholder: "Description" %></li>
	<li><%= f.select(:tag_type, options_for_select([["abc","abc"],["cde","cde"],["fgh","fgh"]], @transaction.tag_type),{prompt:"Tag_type"})%></li>
	<li><%= f.date_field :date_of_transaction, class:'hello' %></li>
	<li><%= f.submit "create_transaction", class: "hello"%></li>
	</ul>
<% end %>